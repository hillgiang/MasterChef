<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.project.cyim.masterchef.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/member"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/member"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/fridge"
        app:layout_constraintBottom_toTopOf="@+id/search"
        android:onClick="member"/>

    <ImageView
        android:id="@+id/fridge"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/fridge"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/member"
        app:layout_constraintBottom_toTopOf="@+id/search" />

    <ImageView
        android:id="@+id/foods"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/foods"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/search"
        app:layout_constraintTop_toBottomOf="@+id/member" />

    <ImageView
        android:id="@+id/search"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/search"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/foods"
        app:layout_constraintTop_toBottomOf="@+id/fridge" />
</android.support.constraint.ConstraintLayout>
